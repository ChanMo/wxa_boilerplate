/* component/backimg/backimg.wxss */
.backimg{
	width: 100%;
	height: 100%;
	position: relative;
	overflow: hidden;
	border-radius: 0rpx;
}
.backimg_img{
	width: 100%;
	min-height: 100%;
	position: absolute;
	z-index: 0;
}
.backimg_view{
	width: 100%;
	height: 100%;
	position: absolute;
	z-index: 1;
}