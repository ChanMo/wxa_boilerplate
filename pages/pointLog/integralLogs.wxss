
.logs_head {
  background-color: #ffce58;
  padding: 0 48rpx;
  padding-bottom: 20rpx;
}

.logs_box {
  position: relative;
  background-color: #fff;
  border-radius: 20rpx 20rpx 0 0;
  top: -20rpx;
  padding: 30rpx;
}

/*  */
.logs_item{
	padding: 20rpx;
	height: 100rpx;
	position: relative;
}
.logs_item text{
	display: block;
}
.logs_item text:nth-child(1){
	font-size: 30rpx;
	font-weight: 500;
	padding-left: 20rpx;
}
.logs_item text:nth-child(2){
	position: absolute;
	right: 0;
	top: 35rpx;
	font-size: 28rpx;
	color: #FFB940;
}
.logs_item text:nth-child(3){
	color: #666666;
	font-size: 20rpx;
	margin-top: 20rpx;
}
.logs_item view{
	position: absolute;
	left: 20rpx;
	top: 36rpx;
	width: 6rpx;
	height: 20rpx;
	background-color: #FFB940;
}